{
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildEnvironment": {
    "NODE_VERSION": "20.x"
  },
  "functions": {
    "api/*.ts": {
      "runtime": "@vercel/node@3.0.0"
    }
  }
}